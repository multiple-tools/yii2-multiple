{
    "name": "umono/yii2-multiple",
    "description": "A common and diverse Toolkit",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","multiple","tools"],
    "license": "MIT",
    "authors": [
        {
            "name": "umono",
            "email": "umono.life@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "umono\\multiple\\": "src"
        }
    },
    "provide": {
        "bower-asset/jquery": "*",
        "bower-asset/bootstrap": "*",
        "bower-asset/inputmask": "*",
        "bower-asset/punycode": "*",
        "bower-asset/typeahead.js": "*",
        "bower-asset/yii2-pjax": "*"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": false
        }
    }
}
